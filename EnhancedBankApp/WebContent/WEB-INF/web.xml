<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EnhancedBankApp</display-name>
  <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.bankapp.controller.LoginServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.bankapp.controller.RegisterServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewBalanceServlet</servlet-name>
        <servlet-class>com.bankapp.controller.ViewBalanceServlet</servlet-class>
