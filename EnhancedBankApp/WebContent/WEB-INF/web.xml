        <url-pattern>/DeleteAccountServlet</url-pattern>
     </servlet-mapping>
     <servlet>
        <servlet-name>WithdrawMoneyServlet</servlet-name>
        <servlet-class>com.bankapp.controller.WithdrawMoneyServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>WithdrawMoneyServlet</servlet-name>
        <url-pattern>/WithdrawMoneyServlet</url-pattern>
     </servlet-mapping>
     <servlet>
        <servlet-name>CheckSuperStatusServlet</servlet-name>
        <servlet-class>com.bankapp.controller.CheckSuperStatusServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>CheckSuperStatusServlet</servlet-name>
        <url-pattern>/CheckSuperStatusServlet</url-pattern>
