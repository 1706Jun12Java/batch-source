<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>BankAssignmentWEB</display-name>
  <servlet>
  	<servlet-name>userLoginServlet</servlet-name>
  	<servlet-class>com.revature.leibniz.bank.servlet.UserLoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>userLoginServlet</servlet-name>
  	<url-pattern>/userlogin</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>CreateAccountUserServlet</servlet-name>
  	<servlet-class>com.revature.leibniz.bank.servlet.CreateAccountUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>CreateAccountUserServlet</servlet-name>
  	<url-pattern>/createAccountAsUser</url-pattern>
  </servlet-mapping>
</web-app>