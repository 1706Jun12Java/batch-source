<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>JDBCBankProject</display-name>
	<servlet>
		<servlet-name>master</servlet-name>
		<servlet-class>Servlets.MasterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>master</servlet-name>
		<url-pattern>/master</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>Servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>signup</servlet-name>
		<servlet-class>Servlets.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>userpage</servlet-name>
		<servlet-class>Servlets.UserPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userpage</servlet-name>
		<url-pattern>/userpage</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>Servlets.LogOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>view</servlet-name>
		<servlet-class>Servlets.ViewServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>view</servlet-name>
		<url-pattern>/view</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>withdraw</servlet-name>
		<servlet-class>Servlets.WithdrawServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>withdraw</servlet-name>
		<url-pattern>/withdraw</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>deposit</servlet-name>
		<servlet-class>Servlets.DepositServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>deposit</servlet-name>
		<url-pattern>/deposit</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>create</servlet-name>
		<servlet-class>Servlets.CreateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>create</servlet-name>
		<url-pattern>/create</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>Servlets.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
</web-app>