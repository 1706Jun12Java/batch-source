<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>FrontControllerDemo</display-name>
  <context-param>
  	<param-name>whereAreWe</param-name>
  	<param-value>IN_THE_AWESOME_JUNGLE</param-value>
  </context-param>
  <servlet>
    <servlet-name>master</servlet-name>
    <servlet-class>com.revature.controller.MasterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>master</servlet-name>
    <url-pattern>/master</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>bear</servlet-name>
    <servlet-class>com.revature.controller.BearServlet</servlet-class>
    <init-param>
    	<param-name>whatAmI</param-name>
    	<param-value>I_AM_A_BEAR</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>bear</servlet-name>
    <url-pattern>/bear</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cave</servlet-name>
    <servlet-class>com.revature.controller.CaveServlet</servlet-class>
    <init-param>
    	<param-name>whatAmI</param-name>
    	<param-value>I_AM_A_CAVE</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>cave</servlet-name>
    <url-pattern>/cave</url-pattern>
  </servlet-mapping>
</web-app>